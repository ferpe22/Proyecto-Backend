<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">

<section class="container">
    <h2>Listado de Productos</h2>
    <p> </p>
    <div class="table-responsive">
        <table class="table table-striped table-bordered table-md justify-middle">
        <thead class="table-dark">
            <tr>
                <th class="text-center">ID</th>
                <th class="text-center">Titulo</th>
            </tr>
        </thead>
        <tbody>
            {{#each products}}
            <tr>
                <td class="text-center">{{this._id}}</td>
                <td class="text-center">{{this.title}}</td>
            </tr>
            {{/each}}
        </tbody>
        </table>
        <div class="container">
            <a href="">Pagina anterior</a>
            <p>Pagina xxx de xxx</p>
            <a href="">Pagina siguiente</a>
        </div>
    </div>
</section>


<script src="/socket.io/socket.io.js"></script>
